
import java.util.*;
import java.util.function.BinaryOperator;

public class Reduce {

  public static <T> T reduceList(List<T> list, BinaryOperator<T> reducer, T defaultValue) {
    if (list.isEmpty()) {
      return defaultValue;
    }

    T result = list.get(0);
    for (int i = 1; i < list.size(); i++) {
      result = reducer.apply(result, list.get(i));
    }
    return result;
  }

}

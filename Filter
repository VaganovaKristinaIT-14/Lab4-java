import java.util.ArrayList;
import java.util.List;
import java.util.function.Predicate;

public class Filter {

  public static <T> List<T> filterList(List<T> list, Predicate<T> tester) {
    List<T> result = new ArrayList<>();
    for (T item : list) {
      if (tester.test(item)) {
        result.add(item);
      }
    }
    return result;
  }

}

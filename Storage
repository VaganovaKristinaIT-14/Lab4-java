
public class Storage<T> {

  private final T item;


  public T getItem() {
    return this.item;
  }

  public Storage(T item) {
    this.item = item;
  }


  public T get(T alternative) {
    if (this.item == null) {
      return alternative;
    }
    return this.item;
  }

  @Override
  public String toString() {
    if (item == null) {
      return "Хранилище: содержит null";
    } else {
      return "Хранилище: содержит " + item.toString();
    }
  }
}
